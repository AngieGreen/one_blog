<!-- Blog Categories Well -->
<div class="well">
    <h4>All 52 Projects</h4>
    <div class="row">
        <ul class="list-unstyled">
            <% @categories.each do |category| %>
                <li class="col-lg-6"><%= link_to category.name, category_path(category[:id]) %></li>
            <% end %>
        </ul>
    </div>
    <!-- /.row -->
</div>

